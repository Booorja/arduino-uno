// Arduino uno

int pulsador = 10;
int estado;
int estadoanterior;
int salida=0;

void setup() {


  Serial.begin(9600); 
  pinMode(pulsador, INPUT); 
   

}

void loop() {
  estado=digitalRead(pulsador);
  if((estado==HIGH)&&(estadoanterior==LOW))
  {
    salida=1-salida;
    
    delay(20);
  }
  estadoanterior=estado;
  if (salida==1){
    Serial.write("1");
  }
  else 
  {
    Serial.write("2");
  }
  
}
